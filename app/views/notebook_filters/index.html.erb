<h1>Listing notebook_filters</h1>

<table>
  <tr>
    <th>Cpu</th>
    <th>Harddisk</th>
    <th>Screen</th>
    <th>Keyword</th>
    <th>Price</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @notebook_filters.each do |notebook_filter| %>
  <tr>
    <td><%= notebook_filter.cpu %></td>
    <td><%= notebook_filter.harddisk %></td>
    <td><%= notebook_filter.screen %></td>
    <td><%= notebook_filter.keyword %></td>
    <td><%= notebook_filter.price %></td>
    <td><%= link_to 'Show', notebook_filter %></td>
    <td><%= link_to 'Edit', edit_notebook_filter_path(notebook_filter) %></td>
    <td><%= link_to 'Destroy', notebook_filter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

